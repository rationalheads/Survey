<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rh.surveydto.CampSections" table="wbs_camp_sections">
		<id name="section_id">
			<column name="Section_Id"/>
			<generator class="assigned"/>
		</id>
		<property name="camp_id">
			<column name="Camp_Id"/>
		</property>
		<property name="section_name">
			<column name="Section_Name"/>
		</property>
		<property name="section_header">
			<column name="Section_Header"/>
		</property>
		<property name="section_image">
			<column name="Section_Image"/>
		</property>
	</class>
</hibernate-mapping>