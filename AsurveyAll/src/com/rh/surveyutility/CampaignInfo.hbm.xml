<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rh.surveydto.CampaignInfo" table="wbs_campaign_info">
		<id name="camp_id">
			<column name="Camp_Id"/>
			<generator class="increment"/>
		</id>
		<property name="cust_id">
			<column name="Cust_Id"/>
		</property>
		<property name="camp_name">
			<column name="Camp_Name"/>
		</property>
		<property name="camp_image">
			<column name="Camp_Image"/>
		</property>
		<property name="camp_header">
			<column name="Camp_Header"/>
		</property>
		<property name="camp_st_page_text">
			<column name="Camp_St_Page_Text"/>
		</property>
		<property name="camp_type">
		    <column name="Camp_Type"></column>
		</property>
		<property name="level_flag">
		    <column name="Level_Flag"></column>
		</property>
				<property name="default_level">
		    <column name="Default_Level"></column>
		</property>
				<property name="qualify_cretirion">
		    <column name="Qualify_Cretirion"></column>
		</property>
				<property name="qualify_q_count">
		    <column name="Qualify_Q_Count"></column>
		</property>
				<property name="success_text">
		    <column name="Success_Text"></column>
		</property>
				<property name="failure_text">
		    <column name="Failure_Text"></column>
		</property>		
	</class>
</hibernate-mapping>
